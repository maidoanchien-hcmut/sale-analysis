{
  "manifest_version": 3,
  "name": "Sale Analysis",
  "version": "0.1.0",
  "description": "Upload chat JSON and analyze sessions",
  "action": {
    "default_popup": "index.html",
    "default_title": "Sale Analysis"
  },
  "permissions": ["storage"],
  "host_permissions": ["http://localhost:8080/*"],
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  }
}
